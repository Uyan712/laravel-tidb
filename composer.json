{
  "name": "colopl/laravel-tidb",
  "description": "Laravel database driver for PingCAP TiDB",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {"name": "Takayasu Oyama", "email": "t-oyama@colopl.co.jp"}
  ],
  "require": {
    "php": ">=8",
    "ext-json": "*",
    "ext-pdo": "*",
    "laravel/framework": "~8|~9"
  },
  "require-dev": {
    "orchestra/testbench": "~6",
    "phpunit/phpunit": "~9"
  },
  "autoload": {
    "psr-4": {
      "Colopl\\TiDB\\": "src/Colopl/TiDB"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Colopl\\TiDB\\Tests\\": "tests/Colopl/TiDB"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Colopl\\TiDB\\TiDBServiceProvider"
      ]
    }
  }
}

